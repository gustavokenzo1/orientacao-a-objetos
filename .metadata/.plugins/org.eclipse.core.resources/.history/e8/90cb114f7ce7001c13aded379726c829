import java.util.Scanner;

public class Ex3 {
	public static void main(String[] args) {
		int num;
		
		Scanner reader = new Scanner(System.in);
		
		do {
			System.out.println("Digite um numero positivo para ser somado ou negativo para sair:");
			num = reader.nextInt();
		} while (num >= 0);
	}
}
